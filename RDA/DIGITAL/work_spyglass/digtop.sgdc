current_design "digtop"

# Virtual Clock for analog/asynchronous inputs
# --------------------------------------------------------
clock -tag VCLK

#PROBABLE CLOCKS:
clock -name "digtop.I_CLKOSC" -domain domain1 -tag OSC
clock -name "digtop.I_SCK" -domain domain2 -tag SCK
