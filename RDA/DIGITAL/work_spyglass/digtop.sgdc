##ASYNCHRONOUS RESETS##
#DEFINE RESETS:
reset -name "digtop.I_TMODE" -value 0 -async
# set no testmode
#set_case_analysis -name digtop.I_TMODE -value 0

#PROBABLE RESETS:
reset -name "digtop.I_PORB"  -value 0 -async
reset -name "digtop.I_RESB"  -value 0 -async
reset -name "digtop.I_CSB"   -value 1 -async
reset -name "digtop.I_TMODE" -value 0 -async

