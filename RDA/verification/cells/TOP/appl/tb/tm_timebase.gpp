define TM_CLK_reset
  
  `comment(TM TM_CLK_reset)
  
    `set_bitfield(name=TMR_SEL_TB_PLL_HOLD val=0)
    `set_bitfield(name=TMR_SEL_TB_PLL_ON   val=0)
  `jtag_write_testreg(name=TMR_SEL_TB block=OSC)
  
    `set_bitfield(name=TMR_VAL_TB_PLL_HOLD val=0)
    `set_bitfield(name=TMR_VAL_TB_PLL_ON   val=0)
  `jtag_write_testreg(name=TMR_VAL_TB block=OSC)
  
    `set_bitfield(name=TMR_OUT_MOSI_MISO_EN_MOSI  val=0)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_SEL_MOSI val=0)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_EN_MISO  val=0)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_SEL_MISO val=0)
  `jtag_write_testreg(name=TMR_OUT_MOSI_MISO block=TOP)
  
enddefine

