define TM_CLK_OSC
  
  `comment(TM TM_CLK_OSC)
  
    `set_bitfield(name=TMR_OUT_MOSI_MISO_EN_MISO  val=1)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_SEL_MISO val=20)
  `jtag_write_testreg(name=TMR_OUT_MOSI_MISO block=TOP)

enddefine

**----------

define TM_CLK_OSC_DIV
  
  `comment(TM TM_CLK_OSC_DIV)
  
    `set_bitfield(name=TMR_OUT_MOSI_MISO_EN_MISO  val=1)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_SEL_MISO val=23)
  `jtag_write_testreg(name=TMR_OUT_MOSI_MISO block=TOP)

