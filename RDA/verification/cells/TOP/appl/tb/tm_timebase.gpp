define TM_CLK_PLL_DIV
  
  `comment(TM TM_CLK_PLL_DIV)
  
    `set_bitfield(name=TMR_VAL_TB_PLL_HOLD val=0)
    `set_bitfield(name=TMR_VAL_TB_PLL_ON   val=1)
  `jtag_write_testreg(name=TMR_VAL_TB block=OSC)
  
    `set_bitfield(name=TMR_SEL_TB_PLL_HOLD val=1)
    `set_bitfield(name=TMR_SEL_TB_PLL_ON   val=1)
  `jtag_write_testreg(name=TMR_SEL_TB block=OSC)
  
    `set_bitfield(name=TMR_OUT_MOSI_MISO_EN_MISO  val=1)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_SEL_MISO val=24)
  `jtag_write_testreg(name=TMR_OUT_MOSI_MISO block=TOP)

enddefine

**----------

define TM_CLK_FILT
  
  `comment(TM TM_CLK_FILT)
  
    `set_bitfield(name=TMR_OUT_MOSI_MISO_EN_MOSI  val=1)
    `set_bitfield(name=TMR_OUT_MOSI_MISO_SEL_MOSI val=18)
  `jtag_write_testreg(name=TMR_OUT_MOSI_MISO block=TOP)
  
enddefine

